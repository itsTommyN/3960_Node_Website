        <tr>
            <th style="padding: 0 20px 0 20px">#</th>
            <th style="padding: 0 20px 0 20px">Description</th>
        </tr>
        <!-- Add data from database -->
        <% let i = 0 %>
        <% for (i = 0; i < list.length; i++) { %>
